[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* POPOVER PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * =============================== */[m
[32m+[m
[32m+[m[32m  var Popover = function (element, options) {[m
[32m+[m[32m    this.init('popover', element, options)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  /* NOTE: POPOVER EXTENDS BOOTSTRAP-TOOLTIP.js[m
[32m+[m[32m     ========================================== */[m
[32m+[m
[32m+[m[32m  Popover.prototype = $.extend({}, $.fn.tooltip.Constructor.prototype, {[m
[32m+[m
[32m+[m[32m    constructor: Popover[m
[32m+[m
[32m+[m[32m  , setContent: function () {[m
[32m+[m[32m      var $tip = this.tip()[m
[32m+[m[32m        , title = this.getTitle()[m
[32m+[m[32m        , content = this.getContent()[m
[32m+[m
[32m+[m[32m      $tip.find('.popover-title')[this.options.html ? 'html' : 'text'](title)[m
[32m+[m[32m      $tip.find('.popover-content > *')[this.options.html ? 'html' : 'text'](content)[m
[32m+[m
[32m+[m[32m      $tip.removeClass('fade top bottom left right in')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hasContent: function () {[m
[32m+[m[32m      return this.getTitle() || this.getContent()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , getContent: function () {[m
[32m+[m[32m      var content[m
[32m+[m[32m        , $e = this.$element[m
[32m+[m[32m        , o = this.options[m
[32m+[m
[32m+[m[32m      content = $e.attr('data-content')[m
[32m+[m[32m        || (typeof o.content == 'function' ? o.content.call($e[0]) :  o.content)[m
[32m+[m
[32m+[m[32m      return content[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , tip: function () {[m
[32m+[m[32m      if (!this.$tip) {[m
[32m+[m[32m        this.$tip = $(this.options.template)[m
[32m+[m[32m      }[m
[32m+[m[32m      return this.$tip[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , destroy: function () {[m
[32m+[m[32m      this.hide().$element.off('.' + this.type).removeData(this.type)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m
[32m+[m[32m /* POPOVER PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  $.fn.popover = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('popover')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('popover', (data = new Popover(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.popover.Constructor = Popover[m
[32m+[m
[32m+[m[32m  $.fn.popover.defaults = $.extend({} , $.fn.tooltip.defaults, {[m
[32m+[m[32m    placement: 'right'[m
[32m+[m[32m  , trigger: 'click'[m
[32m+[m[32m  , content: ''[m
[32m+[m[32m  , template: '<div class="popover"><div class="arrow"></div><div class="popover-inner"><h3 class="popover-title"></h3><div class="popover-content"><p></p></div></div></div>'[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* ==========================================================[m
[32m+[m[32m * bootstrap-affix.js v2.1.1[m
[32m+[m[32m * http://twitter.github.com/bootstrap/javascript.html#affix[m
[32m+[m[32m * ==========================================================[m
[32m+[m[32m * Copyright 2012 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX CLASS DEFINITION[m
[32m+[m[32m  * ====================== */[m
[32m+[m
[32m+[m[32m  var Affix = function (element, options) {[m
[32m+[m[32m    this.options = $.extend({}, $.fn.affix.defaults, options)[m
[32m+[m[32m    this.$window = $(window).on('scroll.affix.data-api', $.proxy(this.checkPosition, this))[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.checkPosition()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Affix.prototype.checkPosition = function () {[m
[32m+[m[32m    if (!this.$element.is(':visible')) return[m
[32m+[m
[32m+[m[32m    var scrollHeight = $(document).height()[m
[32m+[m[32m      , scrollTop = this.$window.scrollTop()[m
[32m+[m[32m      , position = this.$element.offset()[m
[32m+[m[32m      , offset = this.options.offset[m
[32m+[m[32m      , offsetBottom = offset.bottom[m
[32m+[m[32m      , offsetTop = offset.top[m
[32m+[m[32m      , reset = 'affix affix-top affix-bottom'[m
[32m+[m[32m      , affix[m
[32m+[m
[32m+[m[32m    if (typeof offset != 'object') offsetBottom = offsetTop = offset[m
[32m+[m[32m    if (typeof offsetTop == 'function') offsetTop = offset.top()[m
[32m+[m[32m    if (typeof offsetBottom == 'function') offsetBottom = offset.bottom()[m
[32m+[m
[32m+[m[32m    affix = this.unpin != null && (scrollTop + this.unpin <= position.top) ?[m
[32m+[m[32m      false    : offsetBottom != null && (position.top + this.$element.height() >= scrollHeight - offsetBottom) ?[m
[32m+[m[32m      'bottom' : offsetTop != null && scrollTop <= offsetTop ?[m
[32m+[m[32m      'top'    : false[m
[32m+[m
[32m+[m[32m    if (this.affixed === affix) return[m
[32m+[m
[32m+[m[32m    this.affixed = affix[m
[32m+[m[32m    this.unpin = affix == 'bottom' ? position.top - scrollTop : null[m
[32m+[m
[32m+[m[32m    this.$element.removeClass(reset).addClass('affix' + (affix ? '-' + affix : ''))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  $.fn.affix = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('affix')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('affix', (data = new Affix(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.affix.Constructor = Affix[m
[32m+[m
[32m+[m[32m  $.fn.affix.defaults = {[m
[32m+[m[32m    offset: 0[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX DATA-API[m
[32m+[m[32m  * ============== */[m
[32m+[m
[32m+[m[32m  $(window).on('load', function () {[m
[32m+[m[32m    $('[data-spy="affix"]').each(function () {[m
[32m+[m[32m      var $spy = $(this)[m
[32m+[m[32m        , data = $spy.data()[m
[32m+[m
[32m+[m[32m      data.offset = data.offset || {}[m
[32m+[m
[32m+[m[32m      data.offsetBottom && (data.offset.bottom = data.offsetBottom)[m
[32m+[m[32m      data.offsetTop && (data.offset.top = data.offsetTop)[m
[32m+[m
[32m+[m[32m      $spy.affix(data)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* ==========================================================[m
[32m+[m[32m * bootstrap-alert.js v2.1.1[m
[32m+[m[32m * http://twitter.github.com/bootstrap/javascript.html#alerts[m
[32m+[m[32m * ==========================================================[m
[32m+[m[32m * Copyright 2012 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================== */[m
