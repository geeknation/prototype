[32m+[m[32m<?php[m
[32m+[m[32msession_start();[m
[32m+[m[32minclude 'dbconn.inc.php';[m
[32m+[m[32minclude 'api/User.php';[m
[32m+[m[32m$names=$_POST['names'];[m
[32m+[m
[32m+[m[32m$day=$_POST['day'];[m
[32m+[m[32m$month=$_POST['month'];[m
[32m+[m[32m$year=$_POST['year'];[m
[32m+[m[32m$age=$day."/".$month."/".$year;[m
[32m+[m[32m$location=$_POST['location'];[m
[32m+[m[32m$gender=$_POST['gender'];[m
[32m+[m
[32m+[m[32m$user=$_SESSION['user_allow'];[m
[32m+[m[32m$u=new User();[m
[32m+[m[32m$uid=$u->get_user_id($user);[m
[32m+[m[32m$query="UPDATE users SET names='".$names."',yob='".$age."',location='".$location."',gender='".$gender."' WHERE user_id='".$uid."'  ";[m
[32m+[m[32m$result=mysql_query($query);[m
[32m+[m[32mif($result)[m
[32m+[m[32m{[m
[32m+[m	[32mheader("Location: account.php?flag=update_sucess");[m
[32m+[m[32m}[m
[32m+[m[32melse{[m
[32m+[m	[32mheader("Location: account.php?flag=updatefail");[m
[32m+[m[32m}[m
[32m+[m[32mmysql_close();[m
[32m+[m[32m?>[m
\ No newline at end of file[m
