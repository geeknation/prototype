[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32minclude "dbconn.inc.php";[m
[32m+[m
[32m+[m[32minclude 'ptk_functions.php';[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$names=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['names']))));[m
[32m+[m
[32m+[m[32m$username=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['username']))));[m
[32m+[m
[32m+[m[32m$location=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['location']))));[m
[32m+[m
[32m+[m[32m$gender=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['gender']))));[m
[32m+[m
[32m+[m[32m$countrycode=$_POST["countrycode"];[m
[32m+[m
[32m+[m[32m$phone=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['phoneno']))));[m
[32m+[m
[32m+[m[32m$email=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['email']))));[m
[32m+[m
[32m+[m[32m$question=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['question']))));[m
[32m+[m
[32m+[m[32m$answer=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['answer']))));[m
[32m+[m
[32m+[m[32m$password=htmlspecialchars(htmlentities(mysql_escape_string(addslashes($_POST['password']))));[m
[32m+[m
[32m+[m[32m$yob=$_POST['day'].$_POST['month'].$_POST['year'];[m
[32m+[m
[32m+[m[32m$phonenumber=$countrycode.(int)$phone;[m
[32m+[m
[32m+[m[32m$uid=ptk_uniq_id();[m
[32m+[m
[32m+[m[32m$query="INSERT INTO login(user_id,username, password) VALUES('".$uid."','".$username."','".md5($password)."')";[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$query2="INSERT INTO users(user_id,names,location,yob,gender,timecreated) VALUES('".$uid."','".$names."',[m
[32m+[m
[32m+[m[32m'".$location."','".$yob."','".$gender."','".time()."')";[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$query3="INSERT INTO user_phoneno(user_id,phone_number) VALUES('".$uid."','".$phonenumber."')";[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$query4="INSERT INTO user_email(user_id,emailadress) VALUES('".$uid."','".$email."')";[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$query5="INSERT INTO account_recovery(user_id,choosen_question,choosen_answer) VALUES('".$uid."','".$question."','".$answer."')";[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$result=mysql_query($query)or die(mysql_error());[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$result1=mysql_query($query2) or die(mysql_error());[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$result2=mysql_query($query3) or die(mysql_error());[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$result3=mysql_query($query4) or die(mysql_error());[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m$result4=mysql_query($query5) or die(mysql_error());[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mif($result1==true  and $result==true and $result2==true and $result3==true and $result4==true){[m
[32m+[m	[32mheader("location: sucessregistration.php");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32melse{[m
[32m+[m	[32mheader("location: failregistration.php");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m?>[m
