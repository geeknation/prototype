[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT CLASS DEFINITION[m
[32m+[m[32m  * ====================== */[m
[32m+[m
[32m+[m[32m  var dismiss = '[data-dismiss="alert"]'[m
[32m+[m[32m    , Alert = function (el) {[m
[32m+[m[32m        $(el).on('click', dismiss, this.close)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m  Alert.prototype.close = function (e) {[m
[32m+[m[32m    var $this = $(this)[m
[32m+[m[32m      , selector = $this.attr('data-target')[m
[32m+[m[32m      , $parent[m
[32m+[m
[32m+[m[32m    if (!selector) {[m
[32m+[m[32m      selector = $this.attr('href')[m
[32m+[m[32m      selector = selector && selector.replace(/.*(?=#[^\s]*$)/, '') //strip for ie7[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $parent = $(selector)[m
[32m+[m
[32m+[m[32m    e && e.preventDefault()[m
[32m+[m
[32m+[m[32m    $parent.length || ($parent = $this.hasClass('alert') ? $this : $this.parent())[m
[32m+[m
[32m+[m[32m    $parent.trigger(e = $.Event('close'))[m
[32m+[m
[32m+[m[32m    if (e.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m    $parent.removeClass('in')[m
[32m+[m
[32m+[m[32m    function removeElement() {[m
[32m+[m[32m      $parent[m
[32m+[m[32m        .trigger('closed')[m
[32m+[m[32m        .remove()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $.support.transition && $parent.hasClass('fade') ?[m
[32m+[m[32m      $parent.on($.support.transition.end, removeElement) :[m
[32m+[m[32m      removeElement()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  $.fn.alert = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('alert')[m
[32m+[m[32m      if (!data) $this.data('alert', (data = new Alert(this)))[m
[32m+[m[32m      if (typeof option == 'string') data[option].call($this)[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.alert.Constructor = Alert[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT DATA-API[m
[32m+[m[32m  * ============== */[m
[32m+[m
[32m+[m[32m  $(function () {[m
[32m+[m[32m    $('body').on('click.alert.data-api', dismiss, Alert.prototype.close)[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* ============================================================[m
[32m+[m[32m * bootstrap-button.js v2.1.1[m
[32m+[m[32m * http://twitter.github.com/bootstrap/javascript.html#buttons[m
[32m+[m[32m * ============================================================[m
[32m+[m[32m * Copyright 2012 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================ */[m
