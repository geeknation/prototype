[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================ */[m
[32m+[m
[32m+[m
[32m+[m[32m!function($){[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* TYPEAHEAD PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * ================================= */[m
[32m+[m
[32m+[m[32m  var Typeahead = function (element, options) {[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.options = $.extend({}, $.fn.typeahead.defaults, options)[m
[32m+[m[32m    this.matcher = this.options.matcher || this.matcher[m
[32m+[m[32m    this.sorter = this.options.sorter || this.sorter[m
[32m+[m[32m    this.highlighter = this.options.highlighter || this.highlighter[m
[32m+[m[32m    this.updater = this.options.updater || this.updater[m
[32m+[m[32m    this.$menu = $(this.options.menu).appendTo('body')[m
[32m+[m[32m    this.source = this.options.source[m
[32m+[m[32m    this.shown = false[m
[32m+[m[32m    this.listen()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Typeahead.prototype = {[m
[32m+[m
[32m+[m[32m    constructor: Typeahead[m
[32m+[m
[32m+[m[32m  , select: function () {[m
[32m+[m[32m      var val = this.$menu.find('.active').attr('data-value')[m
[32m+[m[32m      this.$element[m
[32m+[m[32m        .val(this.updater(val))[m
[32m+[m[32m        .change()[m
[32m+[m[32m      return this.hide()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , updater: function (item) {[m
[32m+[m[32m      return item[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , show: function () {[m
[32m+[m[32m      var pos = $.extend({}, this.$element.offset(), {[m
[32m+[m[32m        height: this.$element[0].offsetHeight[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      this.$menu.css({[m
[32m+[m[32m        top: pos.top + pos.height[m
[32m+[m[32m      , left: pos.left[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      this.$menu.show()[m
[32m+[m[32m      this.shown = true[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hide: function () {[m
[32m+[m[32m      this.$menu.hide()[m
[32m+[m[32m      this.shown = false[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , lookup: function (event) {[m
[32m+[m[32m      var items[m
[32m+[m
[32m+[m[32m      this.query = this.$element.val()[m
[32m+[m
[32m+[m[32m      if (!this.query || this.query.length < this.options.minLength) {[m
[32m+[m[32m        return this.shown ? this.hide() : this[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      items = $.isFunction(this.source) ? this.source(this.query, $.proxy(this.process, this)) : this.source[m
[32m+[m
[32m+[m[32m      return items ? this.process(items) : this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , process: function (items) {[m
[32m+[m[32m      var that = this[m
[32m+[m
[32m+[m[32m      items = $.grep(items, function (item) {[m
[32m+[m[32m        return that.matcher(item)[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      items = this.sorter(items)[m
[32m+[m
[32m+[m[32m      if (!items.length) {[m
[32m+[m[32m        return this.shown ? this.hide() : this[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this.render(items.slice(0, this.options.items)).show()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , matcher: function (item) {[m
[32m+[m[32m      return ~item.toLowerCase().indexOf(this.query.toLowerCase())[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , sorter: function (items) {[m
[32m+[m[32m      var beginswith = [][m
[32m+[m[32m        , caseSensitive = [][m
[32m+[m[32m        , caseInsensitive = [][m
[32m+[m[32m        , item[m
[32m+[m
[32m+[m[32m      while (item = items.shift()) {[m
[32m+[m[32m        if (!item.toLowerCase().indexOf(this.query.toLowerCase())) beginswith.push(item)[m
[32m+[m[32m        else if (~item.indexOf(this.query)) caseSensitive.push(item)[m
[32m+[m[32m        else caseInsensitive.push(item)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return beginswith.concat(caseSensitive, caseInsensitive)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , highlighter: function (item) {[m
[32m+[m[32m      var query = this.query.replace(/[\-\[\]{}()*+?.,\\\^$|#\s]/g, '\\$&')[m
[32m+[m[32m      return item.replace(new RegExp('(' + query + ')', 'ig'), function ($1, match) {[m
[32m+[m[32m        return '<strong>' + match + '</strong>'[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , render: function (items) {[m
[32m+[m[32m      var that = this[m
[32m+[m
[32m+[m[32m      items = $(items).map(function (i, item) {[m
[32m+[m[32m        i = $(that.options.item).attr('data-value', item)[m
[32m+[m[32m        i.find('a').html(that.highlighter(item))[m
[32m+[m[32m        return i[0][m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      items.first().addClass('active')[m
[32m+[m[32m      this.$menu.html(items)[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , next: function (event) {[m
[32m+[m[32m      var active = this.$menu.find('.active').removeClass('active')[m
[32m+[m[32m        , next = active.next()[m
[32m+[m
[32m+[m[32m      if (!next.length) {[m
[32m+[m[32m        next = $(this.$menu.find('li')[0])[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      next.addClass('active')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , prev: function (event) {[m
[32m+[m[32m      var active = this.$menu.find('.active').removeClass('active')[m
[32m+[m[32m        , prev = active.prev()[m
[32m+[m
[32m+[m[32m      if (!prev.length) {[m
[32m+[m[32m        prev = this.$menu.find('li').last()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      prev.addClass('active')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , listen: function () {[m
[32m+[m[32m      this.$element[m
[32m+[m[32m        .on('blur',     $.proxy(this.blur, this))[m
[32m+[m[32m        .on('keypress', $.proxy(this.keypress, this))[m
[32m+[m[32m        .on('keyup',    $.proxy(this.keyup, this))[m
[32m+[m
[32m+[m[32m      if ($.browser.chrome || $.browser.webkit || $.browser.msie) {[m
[32m+[m[32m        this.$element.on('keydown', $.proxy(this.keydown, this))[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.$menu[m
[32m+[m[32m        .on('click', $.proxy(this.click, this))[m
[32m+[m[32m        .on('mouseenter', 'li', $.proxy(this.mouseenter, this))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , move: function (e) {[m
[32m+[m[32m      if (!this.shown) return[m
[32m+[m
[32m+[m[32m      switch(e.keyCode) {[m
[32m+[m[32m        case 9: // tab[m
[32m+[m[32m        case 13: // enter[m
[32m+[m[32m        case 27: // escape[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 38: // up arrow[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m          this.prev()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 40: // down arrow[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m          this.next()[m
[32m+[m[32m          break[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keydown: function (e) {[m
[32m+[m[32m      this.suppressKeyPressRepeat = !~$.inArray(e.keyCode, [40,38,9,13,27])[m
[32m+[m[32m      this.move(e)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keypress: function (e) {[m
[32m+[m[32m      if (this.suppressKeyPressRepeat) return[m
[32m+[m[32m      this.move(e)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keyup: function (e) {[m
[32m+[m[32m      switch(e.keyCode) {[m
[32m+[m[32m        case 40: // down arrow[m
[32m+[m[32m        case 38: // up arrow[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 9: // tab[m
[32m+[m[32m        case 13: // enter[m
[32m+[m[32m          if (!this.shown) return[m
[32m+[m[32m          this.select()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 27: // escape[m
[32m+[m[32m          if (!this.shown) return[m
[32m+[m[32m          this.hide()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        default:[m
[32m+[m[32m          this.lookup()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m[32m      e.preventDefault()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  , blur: function (e) {[m
[32m+[m[32m      var that = this[m
[32m+[m[32m      setTimeout(function () { that.hide() }, 150)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , click: function (e) {[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m[32m      e.preventDefault()[m
[32m+[m[32m      this.select()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , mouseenter: function (e) {[m
[32m+[m[32m      this.$menu.find('.active').removeClass('active')[m
[32m+[m[32m      $(e.currentTarget).addClass('active')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  /* TYPEAHEAD PLUGIN DEFINITION[m
[32m+[m[32m   * =========================== */[m
[32m+[m
[32m+[m[32m  $.fn.typeahead = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('typeahead')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('typeahead', (data = new Typeahead(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.typeahead.defaults = {[m
[32m+[m[32m    source: [][m
[32m+[m[32m  , items: 8[m
[32m+[m[32m  , menu: '<ul class="typeahead dropdown-menu"></ul>'[m
[32m+[m[32m  , item: '<li><a href="#"></a></li>'[m
[32m+[m[32m  , minLength: 1[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.typeahead.Constructor = Typeahead[m
[32m+[m
[32m+[m
[32m+[m[32m /*   TYPEAHEAD DATA-API[m
[32m+[m[32m  * ================== */[m
[32m+[m
[32m+[m[32m  $(function () {[m
[32m+[m[32m    $('body').on('focus.typeahead.data-api', '[data-provide="typeahead"]', function (e) {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m      if ($this.data('typeahead')) return[m
[32m+[m[32m      e.preventDefault()[m
