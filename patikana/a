[32m+[m[32m<?php[m
[32m+[m[32msession_start();[m
[32m+[m[32minclude 'dbconn.inc.php';[m
[32m+[m[32minclude 'ptk_functions.php';[m
[32m+[m
[32m+[m[32m$username=htmlentities(htmlspecialchars(mysql_real_escape_string(addslashes($_POST['prefusername']))));[m
[32m+[m[32m$pass=htmlentities(htmlspecialchars(mysql_real_escape_string(addslashes($_POST['pass']))));[m
[32m+[m
[32m+[m[32m$user=$_SESSION['user_allow'];[m
[32m+[m[32m$u=new User();[m
[32m+[m[32m$uid=$u->get_user_id($user);[m
[32m+[m
[32m+[m[32m$query="UPDATE login SET username='".$username."',password='".md5($pass)."' WHERE user_id='".$uid."'";[m
[32m+[m
[32m+[m[32m$result=mysql_query($query);[m
[32m+[m
[32m+[m[32m$_SESSION['user_allow']=$username;[m
[32m+[m
[32m+[m[32mif($result)[m
[32m+[m[32m{[m
[32m+[m	[32mheader("Location: account.php?flag=update_sucess");[m
[32m+[m[32m}[m
[32m+[m[32melse{[m
[32m+[m	[32mheader("Location: account.php?flag=updatefail");[m
[32m+[m[32m}[m
[32m+[m[32m?>[m
